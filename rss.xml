---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Abdel Raoof's Blog</title>
    <link>http://blog-olakara.rhcloud.com/</link>
    <atom:link href="http://blog-olakara.rhcloud.com/rss.xml" rel="self" type="application/rss+xml" />
    <description>{{ site.description }}</description>
    <language>en-gb</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts %}
    <item>
      <title>{{ post.title }}</title>
      <link>http://blog-olakara.rhcloud.com/{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>Abdel Raoof</author>
      <guid>http://blog-olakara.rhcloud.com{{ post.id }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}

  </channel>
</rss>